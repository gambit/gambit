# Check whether --enable-debug was given.
if test "${enable_debug+set}" = set; then :
  enableval=$enable_debug; ENABLE_DEBUG=$enableval
else
  ENABLE_DEBUG=no
fi


# Check if debugging of the control flow history is needed.

# Check whether --enable-debug-ctrl-flow-history was given.
if test "${enable_debug_ctrl_flow_history+set}" = set; then :
  enableval=$enable_debug_ctrl_flow_history; ENABLE_DEBUG_CTRL_FLOW_HISTORY=$enableval
else
  ENABLE_DEBUG_CTRL_FLOW_HISTORY=no
fi


if test "$ENABLE_DEBUG_CTRL_FLOW_HISTORY" = yes; then

$as_echo "#define ___DEBUG_CTRL_FLOW_HISTORY /**/" >>confdefs.h

  DEFS_OBJ_DYN_LIB_EXE="$DEFS_OBJ_DYN_LIB_EXE -D___DEBUG_CTRL_FLOW_HISTORY"
  ENABLE_DEBUG=yes
fi

# Check if debugging of the host changes is needed.

# Check whether --enable-debug-host-changes was given.
if test "${enable_debug_host_changes+set}" = set; then :
  enableval=$enable_debug_host_changes; ENABLE_DEBUG_HOST_CHANGES=$enableval
else
  ENABLE_DEBUG_HOST_CHANGES=no
fi


if test "$ENABLE_DEBUG_HOST_CHANGES" = yes; then

$as_echo "#define ___DEBUG_HOST_CHANGES /**/" >>confdefs.h

  DEFS_OBJ_DYN_LIB_EXE="$DEFS_OBJ_DYN_LIB_EXE -D___DEBUG_HOST_CHANGES"
  ENABLE_DEBUG=yes
fi

# Check if debugging the garbage collector is needed.

# Check whether --enable-debug-garbage-collect was given.
if test "${enable_debug_garbage_collect+set}" = set; then :
  enableval=$enable_debug_garbage_collect; ENABLE_DEBUG_GARBAGE_COLLECT=$enableval
else
  ENABLE_DEBUG_GARBAGE_COLLECT=no
fi


if test "$ENABLE_DEBUG_GARBAGE_COLLECT" = yes; then

$as_echo "#define ___DEBUG_GARBAGE_COLLECT /**/" >>confdefs.h

  DEFS_OBJ_DYN_LIB_EXE="$DEFS_OBJ_DYN_LIB_EXE -D___DEBUG_GARBAGE_COLLECT"
  ENABLE_DEBUG=yes
fi

# Turn on debugging.

if test "$ENABLE_DEBUG" = yes; then

$as_echo "#define ___DEBUG /**/" >>confdefs.h

  DEFS_OBJ_DYN_LIB_EXE="$DEFS_OBJ_DYN_LIB_EXE -D___DEBUG"
fi

###############################################################################
#
# Check if the system should keep an activity log.

# Check whether --enable-activity-log was given.
if test "${enable_activity_log+set}" = set; then :
  enableval=$enable_activity_log; ENABLE_ACTIVITY_LOG=$enableval
else
  ENABLE_ACTIVITY_LOG=no
fi


if test "$ENABLE_ACTIVITY_LOG" = yes; then
  CONF_ACTIVITY_LOG="___ACTIVITY_LOG"
else
  CONF_ACTIVITY_LOG="___NO_ACTIVITY_LOG"
fi



###############################################################################
#
# Check if the system is to be profiled.

# Check whether --enable-profile was given.
if test "${enable_profile+set}" = set; then :
  enableval=$enable_profile; ENABLE_PROFILE=$enableval
else
  ENABLE_PROFILE=no
fi


if test "$ENABLE_PROFILE" = yes; then

$as_echo "#define ___PROFILE /**/" >>confdefs.h

  DEFS_OBJ_DYN_LIB_EXE="$DEFS_OBJ_DYN_LIB_EXE -D___PROFILE"
fi

###############################################################################
#
# Check if the system is to be compiled to accumulate coverage statistics.

# Check whether --enable-coverage was given.
if test "${enable_coverage+set}" = set; then :
  enableval=$enable_coverage; ENABLE_COVERAGE=$enableval
else
  ENABLE_COVERAGE=no
fi


###############################################################################
#
# Check if the system is to be compiled with trial run feedback.

# Check whether --enable-feedback1 was given.
if test "${enable_feedback1+set}" = set; then :
  enableval=$enable_feedback1; ENABLE_FEEDBACK1=$enableval
else
  ENABLE_FEEDBACK1=no
fi


# Check whether --enable-feedback2 was given.
if test "${enable_feedback2+set}" = set; then :
  enableval=$enable_feedback2; ENABLE_FEEDBACK2=$enableval
else
  ENABLE_FEEDBACK2=no
fi


###############################################################################
#
# Check if a single C function should be generated for all the Scheme
# functions in a file.  This makes the executable much faster but the
# compilation will be much longer and take much more memory.

# Check whether --enable-single-host was given.
if test "${enable_single_host+set}" = set; then :
  enableval=$enable_single_host; ENABLE_SINGLE_HOST=$enableval
else
  ENABLE_SINGLE_HOST=no
fi


if test "$ENABLE_SINGLE_HOST" = yes; then

$as_echo "#define ___SINGLE_HOST /**/" >>confdefs.h

  DEFS_OBJ_DYN_LIB_EXE="$DEFS_OBJ_DYN_LIB_EXE -D___SINGLE_HOST"
fi

###############################################################################
#
# Check if inline code should be generated for jumps.  It is possible
# that on some platforms this will result in a faster but larger
# executable program.

# Check whether --enable-inline-jumps was given.
if test "${enable_inline_jumps+set}" = set; then :
  enableval=$enable_inline_jumps; ENABLE_INLINE_JUMPS=$enableval
else
  ENABLE_INLINE_JUMPS=no
fi


if test "$ENABLE_INLINE_JUMPS" = yes; then

$as_echo "#define ___INLINE_JUMPS /**/" >>confdefs.h

  DEFS_OBJ_DYN_LIB_EXE="$DEFS_OBJ_DYN_LIB_EXE -D___INLINE_JUMPS"
fi

###############################################################################
#
# Check if expensive GCC optimizations should be used.  This makes the
# executable faster but the compilation will be much longer and take
# much more memory.

# Check whether --enable-gcc-opts was given.
if test "${enable_gcc_opts+set}" = set; then :
  enableval=$enable_gcc_opts; ENABLE_GCC_OPTS=$enableval
else
  ENABLE_GCC_OPTS=no
fi


###############################################################################
#
# Check if GNU GCC specific options should be used.  When the system
# is to be built in a setting where the target C compiler is either
# the GNU GCC or the LLVM GCC it is advisable to avoid using GNU GCC
# specific options that are not accepted by the LLVM GCC compiler
# (this is the case on OS X, where the program "gcc" may be the LLVM
# GCC which is part of Xcode, or the GNU GCC if it was installed
# manually).

# Check whether --enable-gnu-gcc-specific-options was given.
if test "${enable_gnu_gcc_specific_options+set}" = set; then :
  enableval=$enable_gnu_gcc_specific_options; ENABLE_GNU_GCC_SPECIFIC_OPTIONS=$enableval
else
  ENABLE_GNU_GCC_SPECIFIC_OPTIONS=yes
fi


# Check whether --enable-gnu-gcc-no-strict-aliasing was given.
if test "${enable_gnu_gcc_no_strict_aliasing+set}" = set; then :
  enableval=$enable_gnu_gcc_no_strict_aliasing; ENABLE_GNU_GCC_NO_STRICT_ALIASING=$enableval
else
  ENABLE_GNU_GCC_NO_STRICT_ALIASING=yes
fi


###############################################################################
#
# Check the width of Scheme characters.

# Check whether --enable-char-size was given.
if test "${enable_char_size+set}" = set; then :
  enableval=$enable_char_size; CHAR_SIZE=$enableval
else
  CHAR_SIZE=4
fi


if test "$CHAR_SIZE" = 1; then
  CONF_MAX_CHR="0xff"
else
if test "$CHAR_SIZE" = 2; then
  CONF_MAX_CHR="0xffff"
else
  CONF_MAX_CHR="0x10ffff"
fi
fi



###############################################################################
#
# Check the encoding of filesystem paths.

# Check whether --enable-path-encoding was given.
if test "${enable_path_encoding+set}" = set; then :
  enableval=$enable_path_encoding; ENABLE_PATH_ENCODING=$enableval
else
  ENABLE_PATH_ENCODING=no
fi


if test "$ENABLE_PATH_ENCODING" = utf8; then

$as_echo "#define ___PATH_ENCODING_UTF8 /**/" >>confdefs.h

else
  if test "$ENABLE_PATH_ENCODING" = latin1; then

$as_echo "#define ___PATH_ENCODING_LATIN1 /**/" >>confdefs.h

  else
    if test "$ENABLE_PATH_ENCODING" = ucs2; then

$as_echo "#define ___PATH_ENCODING_UCS2 /**/" >>confdefs.h

    else
      if test "$ENABLE_PATH_ENCODING" = ucs4; then

$as_echo "#define ___PATH_ENCODING_UCS4 /**/" >>confdefs.h

      else
        if test "$ENABLE_PATH_ENCODING" = wchar; then

$as_echo "#define ___PATH_ENCODING_WCHAR /**/" >>confdefs.h

        else
          if test "$ENABLE_PATH_ENCODING" = native; then

$as_echo "#define ___PATH_ENCODING_NATIVE /**/" >>confdefs.h

          fi
        fi
      fi
    fi
  fi
fi

###############################################################################
#
# Check the size of the type "void*".  On many systems it is the same
# size as "long".  If this isn't the case then define
# ___VOIDSTAR_WIDTH appropriately.



{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for grep that handles long lines and -e" >&5
$as_echo_n "checking for grep that handles long lines and -e... " >&6; }
if ${ac_cv_path_GREP+:} false; then :
  $as_echo_n "(cached) " >&6
else
  if test -z "$GREP"; then
  ac_path_GREP_found=false
  # Loop through the user's path and test for each of PROGNAME-LIST
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH$PATH_SEPARATOR/usr/xpg4/bin
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_prog in grep ggrep; do
    for ac_exec_ext in '' $ac_executable_extensions; do
      ac_path_GREP="$as_dir/$ac_prog$ac_exec_ext"
      as_fn_executable_p "$ac_path_GREP" || continue
# Check for GNU ac_path_GREP and select it if it is found.
  # Check for GNU $ac_path_GREP
case `"$ac_path_GREP" --version 2>&1` in
*GNU*)
  ac_cv_path_GREP="$ac_path_GREP" ac_path_GREP_found=:;;
*)
  ac_count=0
  $as_echo_n 0123456789 >"conftest.in"
  while :
  do
    cat "conftest.in" "conftest.in" >"conftest.tmp"
    mv "conftest.tmp" "conftest.in"
    cp "conftest.in" "conftest.nl"
    $as_echo 'GREP' >> "conftest.nl"
    "$ac_path_GREP" -e 'GREP$' -e '-(cannot match)-' < "conftest.nl" >"conftest.out" 2>/dev/null || break
    diff "conftest.out" "conftest.nl" >/dev/null 2>&1 || break
    as_fn_arith $ac_count + 1 && ac_count=$as_val
    if test $ac_count -gt ${ac_path_GREP_max-0}; then
      # Best one so far, save it but keep looking for a better one
      ac_cv_path_GREP="$ac_path_GREP"
      ac_path_GREP_max=$ac_count
    fi
    # 10*(2^10) chars as input seems more than enough
    test $ac_count -gt 10 && break
  done
  rm -f conftest.in conftest.tmp conftest.nl conftest.out;;
esac

      $ac_path_GREP_found && break 3
    done
  done
  done
IFS=$as_save_IFS
  if test -z "$ac_cv_path_GREP"; then
    as_fn_error $? "no acceptable grep could be found in $PATH$PATH_SEPARATOR/usr/xpg4/bin" "$LINENO" 5
  fi
else
  ac_cv_path_GREP=$GREP
fi

fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_path_GREP" >&5
$as_echo "$ac_cv_path_GREP" >&6; }
 GREP="$ac_cv_path_GREP"


